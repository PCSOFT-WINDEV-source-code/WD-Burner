       name : BTN_Burn
       identifier : 0x219cbec312233bb2
       internal_properties : 
       properties :
        x : 97
        y : 3
        width : 64
        height : 64
       type : 4
       code_elements :
        type_code : 4
        p_codes :
         -
           internal_properties : CAAAAAgAAABt1qmqaP9/JHMmlTwc8Za9wKmSYybc/wJ+98h+UyheoaWyI4Fshy2jeUSC7BuKqUJb257CaI4BkYbQ2AjxI+KWapHu4PS5zQMCh6ghd922hhhzMLwz3o9SNF1zX1ik94rOveEaO8a0JMd/
           code : |1-
            //Retrieves the name of the compilation for the burn process
            scListFil   is string //List of files to burn
            sTreePath is string
            i			 is int
            
            // According to the type of compilation
            SWITCH WIN_Compile.gnCompileType
            	CASE CompileData // Data CD/DVD
            		// Transforms the list of files into character string
            		HReadFirst(BurnerFile,BurnerFileID)
            		WHILE NOT HOut()
            			IF NOT BurnerFile.AlreadyBurned THEN
            				// Replace the Root word by \
            				IF BurnerFile.TreeViewPath = “Root” THEN
            					sTreePath = “\”
            				ELSE
            					sTreePath = Replace(BurnerFile.TreeViewPath,“Root”,“”)	
            				END
            				
            				// Replace the TAB characters by \
            				sTreePath	= Replace(sTreePath,TAB,“\”)
            				// Add the name of the file with its extension
            				sTreePath	= CompleteDir(sTreePath)+fExtractPath(BurnerFile.FullPath,fFileName)+fExtractPath(BurnerFile.FullPath,fExtension)
            				scListFil += BurnerFile.FullPath+“;”+sTreePath+CR
            			END
            			HReadNext(BurnerFile,BurnerFileID)
            		END
            	CASE CompileAudio
            		FOR i = 1 _TO_ WIN_Compile.TABLE_Audio..Occurrence
            			scListFil += WIN_Compile.TABLE_Audio.COL_Access_Path[i]+“;”+“”+CR
            		END
            	OTHER CASE
            		
            END
            
            // Start the window for burning (run the burner test, add files, burn)
            IF NOT BurnThe_files(scListFil,WIN_Compile.gnCompileType,WIN_Compile.EDT_Name_of_CD_DVD) THEN
            	Info(“The burn process was not performed”)
            END
