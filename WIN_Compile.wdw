        code : |1+
         nbSelected 		is int
         i 					is int = 1
         nCurrentSubscript  	is int
         
         
         // Delete a file
         IF _EVE.wParam = VK_DELETE THEN
         	nbSelected = TableSelectCount(TABLE_Table2)
         	
         	WHILE i <= nbSelected
         		nCurrentSubscript = TableSelect(TABLE_Table2,i)
         		IF nCurrentSubscript <> -1 THEN
         			// Search in the file on a composite key
         			HReadSeek(BurnerFile,FullPathTreeViewPath,[TABLE_Table2.COL_CompletePathHD[nCurrentSubscript],TreeSelect(TREE_Treeview2)])
         			IF HFound() THEN
         				// If the file is not already burned
         				IF NOT BurnerFile.AlreadyBurned THEN
         					i++ // an additional file
         					HDelete(BurnerFile)
         					gxNbEstimatedBytes -= COL_Size[nCurrentSubscript]
         					gxNbOfFilesOnCD_DVD--
         				ELSE
         					// deselects the file
         					TableSelectMinus(TABLE_Table2,nCurrentSubscript)
         					// refresh the number of selected files
         					nbSelected = TableSelectCount(TABLE_Table2)
         					IF i > 1 THEN i-- ELSE i=1
         				END
         			END
         		END
         	END
         	FOR i = 1 TO nbSelected
         		nCurrentSubscript = TableSelect(TABLE_Table2,i)
         		TableDelete(TABLE_Table2,nCurrentSubscript)
         	END
         	// Refresh the data in the status bar
         	DisplayStatusBar(“Del”)
         END
         
