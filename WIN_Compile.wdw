      // Summary: <specify the action of the procedure>
      // Syntax:
      //_cboxCompileWin ( [<pCompileType> is int])
      //
      // Parameters:
      //	pCompileType (integer - default value=2): Specify the type of compilation: DataCompile or AudioCompile
      // Return value:
      // 	None
      //
      // Info
      // A block corresponds to 2048 bytes on a data CD/DVD and to 2352 bytes on an audio CD
      Procedure _cboxCompileWin(pCompileType is int = CompileData )
      
      
      //By default, the name of the compilation
      EDT_Name_of_CD_DVD = gCompileName // Compile1
      
      SWITCH pCompileType
      	CASE CompileData // Data compilation
      		//Display the relevant plane
      		WIN_Compile..Plane = 1
      		IF BurnerStatus() = burnerInactive THEN
      			xTotalcapacity     = (BurnerMediaInfo(mediaTotalSize)*0n2)/1024
      			xFreespace	   = (BurnerMediaInfo(mediaFreeSize)*0n2)/1204
      			xSpaceUsed	   = (BurnerMediaInfo(mediaSizeUsed)*0n2)/1024
      		END
      		IF xSpaceUsed > 0 THEN
      			ReadDataOnDisk()
      		END
      		SBC_NbDirFile	   = “0 Directory and 0 File”
      		// Define the exention of the filtered files.
      		gsExtStr = “*.*”
      	CASE CompileAudio // Audio compilation
      		//Display the relevant plane
      		WIN_Compile..Plane = 2
      		IF BurnerStatus() = burnerInactive THEN
      			xTotalcapacity     = ((BurnerMediaInfo(mediaTotalSize)*0n2352)/1024)/1204
      			xFreespace	   = ((BurnerMediaInfo(mediaFreeSize)*0n2352)/1024)/1024
      			xSpaceUsed	   = ((BurnerMediaInfo(mediaSizeUsed)*0n2352)/1024)/1024
      		END
      		SBC_NbDirFile = “0 track”
      		// Define the exention of the filtered files.
      		gsExtStr = “*.WAV”
      	OTHER CASE
      		// no other case
      END
      
      //Initialize the progress bar
      PROGBAR_TotalContent..MinValue = 0
      IF xTotalcapacity > 0 THEN
      	PROGBAR_TotalContent..MaxValue = Round(xTotalcapacity,2)
      ELSE
      	// If no CD/DVD, initialize to 702 MB
      	PROGBAR_TotalContent..MaxValue = 702
      END
      //Display the space used on disk
      PROGBAR_TotalContent..Value = Round(xSpaceUsed,2)
      
      // Display in the status bar
      SBC_Capity 				= “Total space: ”+Round(xTotalcapacity,2)+“ Mb”
      SBC_OccupiedSpace 		= “Space used: ”+Round(xSpaceUsed,2)+“ Mb”
      SBC_TotalSpaceEstimate 	= “Estimated Space: 0 Mb”
      
      // Enables the buttons for burning
      WIN_Main.TBAR_HandTools.BTN_CloseDraw..State 			= Active
      WIN_Main.TBAR_HandTools.BTN_Eject..State 				= Active
      WIN_Main.GrpCD..State 									= Active
      WIN_Main.GrpNew..State 								= Grayed
