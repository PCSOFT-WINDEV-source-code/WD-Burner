      //
      // Callback procedure
      Procedure _ListFileAndDirToAdd(pPath, pFileName, pChange, pPointer=0)
      
      sPat is string // path of the file
      
      // Add the files and directories found
      IF pChange = flChangeDir THEN //
      	// add a new directory
      	gxNbOfDirOnCD_DVD++
      	//Retrieve the path to display in the branch
      	sPat = pPath[[Length(gsBasePath) TO]]
      	sPat = Replace(sPat,“\”,TAB)
      	// Build the treeview branch for the new directory.
      	gsCurrentBranch = gsBranch+Middle(sPat,1,Length(sPat)-1)
      	// Add the branch to the treeview
      	TreeAdd(TREE_Treeview2,gsCurrentBranch,tvDefault,tvDefault,pPath)
      END
      // add a file
      gxNbOfFilesOnCD_DVD++
      // Add to the data file of the tree structure.
      HReset(BurnerFile)
      BurnerFile.TreeViewPath 	= gsCurrentBranch
      BurnerFile.FullPath 	= pPath+pFileName
      BurnerFile.FilDate       	= fDate(pPath+pFileName,“”,fModify)
      BurnerFile.FileName    	= fExtractPath(pPath+pFileName,fFileName)
      BurnerFile.FilSize	 	= fSize(pPath+pFileName)
      BurnerFile.Exten		  	= fExtractPath(pPath+pFileName,fExtension)
      HAdd(BurnerFile)
      // add the number of estimated bytes on future CD/DVD
      gxNbEstimatedBytes += BurnerFile.FilSize
      
