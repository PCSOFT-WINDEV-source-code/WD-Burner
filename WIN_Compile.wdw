        code : |1+
         //Display the files available on the future CD/DVD
         spath 		is string = TreeSelect(TREE_Treeview2)
         sSearchKey 	is string
         sCondition 		is string
         nRowSubscript     is int
         
         
         // Deletes the table content
         TableDeleteAll(TABLE_Table2)
         // Filter condition
         sCondition=StringBuild(“TreeViewPath = '%1' ”,spath)
         // Search in the file
         sSearchKey = HFilter(BurnerFile,sCondition)
         HReadFirst(BurnerFile,sSearchKey)
         WHILE NOT HOut()
         	nRowSubscript++
         	TableAddLine(TABLE_Table2,BurnerFile.FileName,BurnerFile.Exten,BurnerFile.FilSize,BurnerFile.FilDate,BurnerFile.FullPath)
         	IF NOT BurnerFile.AlreadyBurned THEN
         		TABLE_Table2[nRowSubscript]..Color = iBlack
         	ELSE
         		TABLE_Table2[nRowSubscript]..Color = iLightGray
         	END
         	HReadNext(BurnerFile,sSearchKey)
         END
         
         
         
         
