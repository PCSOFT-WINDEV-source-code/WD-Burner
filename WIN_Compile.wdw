     name : _ListDirFileCD_DVD
     internal_properties : 
     procedure_id : 1269098926913957263
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //_ListDirFileCD_DVD (<pPath>, <pNameOfFile>, <pChange> [, <pPointer>])
      //
      // Parameters:
      //	pPath: <specify the role of pPath>
      //	pNameOfFile: <specify the role of pNameOfFile>
      //	pChange: <specify the role of pChange>
      //	pPointer (default value=0): <specify the role of pPointer>
      // Return value:
      // 	None
      //
      // Note:
      // 	Read the directories and files found on disk
      Procedure _ListDirFileCD_DVD(pPath, pFileName, pChange, pPointer=0)
      
      sPat is string // path of the file
      s is string
      i is int
      sBranch is string
      
      
      // Add the files and directories found
      IF pChange = flChangeDir THEN //
      	// add a new directory
      	gxNbOfDirOnCD_DVD++
      	//Retrieve the path to display in the branch
      	sPat = pPath[[Length(gsBasePathCD_DVD) TO]]
      	sPat = Replace(sPat,“\”,TAB)
      	// Build the treeview branch for the new directory.
      	gsCurrentBranchCD_DVD = gsBranchCD_DVD+Middle(sPat,1,Length(sPat)-1)
      	// Add the branch to the treeview
      	TreeAdd(TREE_Treeview2,gsCurrentBranchCD_DVD,tvDefault,tvDefault,pPath)
      	// Color the branches of the CD/DVD in light gray
      	FOR EACH string s of gsCurrentBranchCD_DVD SEPAREE by TAB
      		i++
      		IF i > 1 THEN
      			// All the other branches in light gray
      			sBranch += TAB+s
      			TREE_Treeview2[sBranch]..Color = iLightGray
      		ELSE
      			// The "Root" branch remains in black
      			sBranch = s
      		END
      	END
      	
      END
      	
      // add a file
      gxNbOfFilesOnCD_DVD++
      // Add to the data file of the tree structure.
      HReset(BurnerFile)
      BurnerFile.TreeViewPath 	= gsCurrentBranchCD_DVD
      BurnerFile.FullPath 	= pPath+pFileName
      BurnerFile.FilDate       	= fDate(pPath+pFileName,“”,fModify)
      BurnerFile.FileName    	= fExtractPath(pPath+pFileName,fFileName)
      BurnerFile.FilSize	 	= fSize(pPath+pFileName)
      BurnerFile.Exten		  	= fExtractPath(pPath+pFileName,fExtension)
      BurnerFile.AlreadyBurned		= Yes
      HAdd(BurnerFile)
      // add the number of estimated bytes on future CD/DVD
      gxNbEstimatedBytes += BurnerFile.FilSize
