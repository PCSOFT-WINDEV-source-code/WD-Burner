      Procedure WIN_Compile(gCompileName = “Compile1”, gnCompileType is int = CompileData)
      GLOBAL
      	// Variables for filling the treeview1
      	gsBranch 				is string			// Current branch in treeview1
      	gsBasePath 			is string			// Path of the base branch
      	gsCurrentBranch		is string			// Path of the branch curently used
      	// Variables used to fill the treeview2 when a data CD/DVD contains data
      	gsBranchCD_DVD 		is string			// Current branch in treeview2
      	gsBasePathCD_DVD 	is string			// Path of the base branch of CD/DVD
      	gsCurrentBranchCD_DVD	is string			// Path of the branch curently used of CD/DVD
      	// Variables for choosing the extension of the files to filter
      	gsExtStr		   			is string = “*.*” 	// Extension to filter
      	// Other variables for the disk information
      	gxNbEstimatedBytes 		is numeric		// Total number of bytes in the files to burn on CD/DVD
      	gxNbOfDirOnCD_DVD		is numeric		// Number of directories on future CD/DVD
      	gxNbOfFilesOnCD_DVD 	is numeric		// Number of files on future CD/DVD
      	xTotalcapacity 			is numeric = 0	// Total capacity of disk
      	xFreespace 			is numeric = 0	// Free space on disk
      	xSpaceUsed 			is numeric = 0	// Space occupied on disk
      	
      
      // Branch the events for Drag&Drop between the tables
      // Definit the source and the target
      TABLE_Table1..DndSource = dndProgram
      TABLE_Table2..DndTarget  = dndProgram
      TABLE_Audio..DndTarget   = dndProgram
      
      // Define the events 
      DnDEvent(“OnStartDrag”,TABLE_Table1,dndBeginDrag)
      // Event of table_table2
      DnDEvent(“OnMouseOverTable2”,TABLE_Table2,dndDragOver)
      DnDEvent(“OnDropTable2”,TABLE_Table2,dndDrop)
      // Event of table_Audio
      DnDEvent(“OnMouseOverAudioTable”,TABLE_Audio,dndDragOver)
      DnDEvent(“OnDropAudioTable”,TABLE_Audio,dndDrop)
      
      // Create the file (it is cleared if it exists)
      HCreation(BurnerFile)
