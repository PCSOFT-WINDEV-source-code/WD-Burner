      // Syntax:
      //OnDropTable2 ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      // Example:
      // Indicate an example.
      //
      Procedure OnDropTable2()
      sRes 		is string	// Copied file or directory
      sComStr 		is string	// Access path to the file or directory
      sext 		is string	// Extension of the file
      sFileDirName 	is string	// Name of the copied directory
      sFil		is string	// Result of the copied elements (Drag&Drop) (list of files and/or directories)
      
      
      sFil = DnDGetData(CF_TEXT)
      
      FOR EACH string sRes of sFil SEPAREE by CR
      	// Extension of the file
      	sext 		= ExtractString(sRes,2,TAB)
      	// Full access path to the file
      	sComStr 		= ExtractString(sRes,5,TAB)
      	// Name of the copied directory
      	sFileDirName 	= Middle(sRes,Position(sRes,“]”,1)+1)
      	sFileDirName 	= NoSpace(ExtractString(sFileDirName,1,TAB))
      	
      	// Keep the selected branch
      	IF TreeSelect(TREE_Treeview2) <> “” THEN
      		gsBranch 		= TreeSelect(TREE_Treeview2)
      	ELSE
      		gsBranch 		= “Root”
      	END
      	
      	// Find the extension of the file
      	IF fExtractPath(sComStr,fExtension) <> “” THEN
      		TableAdd(TABLE_Table2,sRes)
      		// Add to the data file of the tree structure.
      		HReset(BurnerFile)
      		BurnerFile.TreeViewPath = gsBranch
      		BurnerFile.FullPath = sComStr
      		BurnerFile.FilDate       = fDate(sComStr,“”,fModify)
      		BurnerFile.FileName    = fExtractPath(sComStr,fFileName)
      		BurnerFile.FilSize	  = fSize(sComStr)
      		BurnerFile.Exten		  = fExtractPath(sComStr,fExtension)
      		HAdd(BurnerFile)
      		// Calculate the space used
      		gxNbEstimatedBytes += BurnerFile.FilSize
      		// Add an additional file to the counter
      		gxNbOfFilesOnCD_DVD++
      	ELSE
      		// It's a directory. Create the branches
      		// Data CD/DVD. Create a branch and list all the files found in the directory.
      		TableDeleteAll(TABLE_Table2)
      		IF sext = “” THEN // it's a directory
      			// Retrieves the access path to the directory
      			gsBasePath = ExtractString(sComStr,1,Charact(0))
      			// Build the start branch
      			gsBranch += TAB + sFileDirName
      			// The current branch is identical to the first branch of the copied element (at the beginning)
      			gsCurrentBranch = gsBranch
      			// Add the first branch
      			TreeAdd(TREE_Treeview2,gsBranch)
      			// List of directories and list of files found in the copied directory
      			fListFile(ExtractString(sComStr,1,Charact(0))+“*.*”,_ListFileAndDirToAdd)
      		END	
      	END
      END
      
      // Expand the treeview and select the first created branch
      TreeExpand(TREE_Treeview2,gsBranch)
      TreeSelectPlus(TREE_Treeview2,gsBranch)
      ExecuteProcess(TREE_Treeview2,trtSelection)
      
      // Display the information in the status bar
      DisplayStatusBar()
      
      
      
