        code : |1+
         // Move a track up
         subscript is int = TableSelect(TABLE_Audio)	//subscript of the selected track
         sSourceString is string					// All the table elements to move
         
         IF subscript <> -1 _AND_ subscript-1 <> 0 THEN
         	sSourceString = TABLE_Audio.COL_Name[subscript]+TAB+TABLE_Audio.COL_Partieheure[subscript]+TAB+TABLE_Audio.COL_Size[subscript]+TAB+TABLE_Audio.COL_Access_Path[subscript]
         	// Move from the row below to the current row
         	TableModify(TABLE_Audio,subscript+TAB+TABLE_Audio.COL_Name[subscript-1]+TAB+TABLE_Audio.COL_Partieheure[subscript-1]+TAB+TABLE_Audio.COL_Size[subscript-1]+TAB+TABLE_Audio.COL_Access_Path[subscript-1],subscript)
         	// Move from the saved row to the row below
         	TableModify(TABLE_Audio,subscript-1+TAB+sSourceString,subscript-1)
         	// select the same row moved
         	TableSelectPlus(TABLE_Audio,subscript-1)
         END
        type : 18
