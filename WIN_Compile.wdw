      // Summary: <specify the action of the procedure>
      // Syntax:
      //_FillTreeView ( [<sTreeViewName> is string])
      //
      // Parameters:
      //	sTreeViewName (string - default value="TREE_TreeView1"): Name of the treeview to fill
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //// Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be run in a thread (no need to call ThreadExecute), without using Hyper File
      // 
      
      Procedure _FillTreeView(sTreeViewName is string = “TREE_TreeView1”)
      // Local variables
      sDiskLetter 	is string	// Letter for the disks (b:,c:,d:,...,z:)
      nDiskInfo 	is int	// Type of disk found
      nI 				is int	// Loop Variable
      sVolName 		is string	// Name of the volume
      
      
      // Root of the explorer
      TreeAdd(sTreeViewName,“Desktop”,IMG_Desktop,IMG_Desktop,1)
      TreeAdd(sTreeViewName,“Desktop”+TAB+“My Documents”,IMG_Desktop,IMG_Desktop,CompleteDir(SysDir(srMyDocuments)))
      TreeAdd(sTreeViewName,“Desktop”+TAB+“Computer”,IMG_Desktop,IMG_Desktop,2)
      
      // Pass the current branch
      gsBranch = “Desktop”+TAB+“My Documents”
      // Lists the folders on "\My Documents" directory
      fListDirectory(SysDir(srMyDocuments) , _ListDirectory)
      
      //Browse the letters from B to Z
      FOR nI = 66 TO 90
      	sDiskLetter = Charact(nI)+“:”
      	nDiskInfo = fDriveInfo(sDiskLetter,fdDriveType)
      	SWITCH nDiskInfo
      		CASE fdFixed
      			sVolName = fDriveInfo(sDiskLetter,fdVolumeName)
      			IF  sVolName = “” THEN
      				sVolName = “Local disk”
      			END
      			TreeAdd(sTreeViewName,“Desktop”+TAB+“Computer”+TAB+sDiskLetter+“ ”+sVolName,IMG_Disk,IMG_Disk,Charact(nI)+“:\”)
      			// Pass the current branch
      			gsBranch = “Desktop”+TAB+“Computer”+TAB+sDiskLetter+“ ”+sVolName
      			// Lists the folders on "\My Documents" directory
      			fListDirectory(CompleteDir(sDiskLetter) , _ListDirectory)
      			ListAdd(COMBO_Combo1,gImage(“LASER2-16-5.bmp”)+“ ”+sDiskLetter+“ ”+sVolName+gLink(sDiskLetter+“ ”+sVolName))
      		CASE fdRemote
      			TreeAdd(sTreeViewName,“Desktop”+TAB+“Computer”+TAB+sDiskLetter+“ Network drive”,IMG_NetworkDisk,IMG_NetworkDisk,Charact(nI)+“:\”)
      			// Pass the current branch
      			gsBranch = “Desktop”+TAB+“Computer”+TAB+sDiskLetter+“ Network drive”
      			// Lists the folders on "\My Documents" directory
      			fListDirectory(CompleteDir(sDiskLetter) , _ListDirectory)
      			ListAdd(COMBO_Combo1,gImage(“SMP05051-16-5.png”)+“ ”+sDiskLetter+“ Network drive”+gLink(sDiskLetter+“ Network drive”))
      		CASE fdCDRom
      			TreeAdd(sTreeViewName,“Desktop”+TAB+“Computer”+TAB+sDiskLetter+“ CD/DVD-ROM”,IMG_CDROM,IMG_CDROM,Charact(nI)+“:\”)
      			ListAdd(COMBO_Combo1,gImage(“VST13202-16-5.png”)+“ ”+sDiskLetter+“ CD/DVD-ROM”+gLink(sDiskLetter+“ CD/DVD-ROM”))
      		OTHER CASE
      			
      	END
      END
      // Expand the desktop
      TreeExpand(TREE_TreeView1,“Desktop”)
      // Select c:
      ListSelectPlus(COMBO_Combo1,1)
      ExecuteProcess(COMBO_Combo1,trtSelection)
