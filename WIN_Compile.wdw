        internal_properties : CAAAAAgAAADj1fohfb4+Jr98v5rcJ9hZm61snGgt8xOHatv4yAUPV9EU9sCbOFlN2LOTlLD+hS7/SupgEFRGiBohRFl0/IroQSVYr+m43KwAol6oio5Ejj8NDDd+5yqrLCy5EAvtQ+jZqXt1Vo/2yowu34Q=
        code : |1+
         // Delete a branch and its content
         spath 		is string = TreeSelect(TREE_Treeview2)
         sSearchKey 	is string
         sCondition 		is string
         
         IF _EVE.wParam = VK_DELETE THEN
         	// Delete the branches colored in black only (the branches in light gray are already burned)
         	IF TREE_Treeview2[spath]..Color <> iLightGray THEN
         		IF spath <> “Root” THEN
         			spath 		= TreeSelect(TREE_Treeview2)
         			IF TREE_Treeview2[spath]..Color = iLightGray THEN Info(“Already burned”)
         			sCondition		= StringBuild(“TreeViewPath ]= '%1' ”,spath)
         			sSearchKey 	= HFilter(BurnerFile,sCondition)
         			gxNbOfDirOnCD_DVD--
         			HReadFirst(BurnerFile,sSearchKey)
         			WHILE NOT HOut()
         				gxNbOfFilesOnCD_DVD--
         				gxNbEstimatedBytes -= BurnerFile.FilSize
         				HDelete(BurnerFile)
         				HReadNext(BurnerFile,sSearchKey)
         			END
         			TableDeleteAll(TABLE_Table2)
         			TreeDelete(TREE_Treeview2,spath)
         		END
         	END
         END
         
         //Refresh the display of the status bar
         DisplayStatusBar(“Del”)
