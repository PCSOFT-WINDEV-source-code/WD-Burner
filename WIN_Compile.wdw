      // Summary: <specify the action of the procedure>
      // Syntax:
      //DisplayStatusBar ( [<mode> is string])
      //
      // Parameters:
      // 	mode (string - default value="Add"): <specify the role of mode>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procedure DisplayStatusBar(sMode is string = “Add”)
      
      // Estimated number of MB on disk
      SBC_TotalSpaceEstimate = “Total estimated space: ”+NumToString(Round((gxNbEstimatedBytes/1048576),2))+“ Mb”
      
      IF sMode = “Add” THEN
      	// Visual representation of the future space taken on disk
      	PROGBAR_TotalContent..Value = xSpaceUsed + Round((gxNbEstimatedBytes/1048576),2)
      ELSE
      	PROGBAR_TotalContent..Value = xSpaceUsed + Round((gxNbEstimatedBytes/1048576),2)
      END
      
      SWITCH gnCompileType
      	CASE CompileData
      		// Display the number of directories and files
      		IF gxNbOfDirOnCD_DVD > 1 THEN
      			IF gxNbOfFilesOnCD_DVD > 1 THEN
      				SBC_NbDirFile = NumToString(gxNbOfDirOnCD_DVD)+“ directories and ”+NumToString(gxNbOfFilesOnCD_DVD)+“ files”
      			ELSE
      				SBC_NbDirFile = NumToString(gxNbOfDirOnCD_DVD)+“ directories and ”+NumToString(gxNbOfFilesOnCD_DVD)+“ file”
      			END
      			
      		ELSE
      			IF gxNbOfFilesOnCD_DVD > 1 THEN
      				SBC_NbDirFile = NumToString(gxNbOfDirOnCD_DVD)+“ directory and ”+NumToString(gxNbOfFilesOnCD_DVD)+“ files”
      			ELSE
      				SBC_NbDirFile = NumToString(gxNbOfDirOnCD_DVD)+“ directory and ”+NumToString(gxNbOfFilesOnCD_DVD)+“ file”
      			END
      			
      		END
      	CASE CompileAudio
      		IF gxNbOfFilesOnCD_DVD > 1 THEN
      			SBC_NbDirFile = NumToString(gxNbOfFilesOnCD_DVD)+“ tracks”
      		ELSE
      			SBC_NbDirFile = NumToString(gxNbOfFilesOnCD_DVD)+“ track”
      		END
      	OTHER CASE
      		
      END
