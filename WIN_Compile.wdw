        code : |1-
         // delete a track
         nI is int = TableSelect(TABLE_Audio)
         
         nbSelected 	is int
         i 				is int
         nCurrentSubscript  is int
         
         
         // Delete a file
         IF _EVE.wParam = VK_DELETE THEN
         	nbSelected = TableSelectCount(TABLE_Audio)
         	FOR i = 1 TO nbSelected
         		nCurrentSubscript = TableSelect(TABLE_Audio,i)
         		// Search in the file on a composite key
         		gxNbEstimatedBytes -= TABLE_Audio.COL_Size[nCurrentSubscript]
         		gxNbOfFilesOnCD_DVD--
         	END
         	IF nbSelected = TABLE_Audio..Occurrence THEN 
         		TableDeleteAll(TABLE_Audio)
         	ELSE
         		FOR i = 1 _TO_ nbSelected
         			nCurrentSubscript = TableSelect(TABLE_Audio,i)
         			TableDelete(TABLE_Audio,nCurrentSubscript)
         		END
         		FOR i = 1 _TO_ TABLE_Audio..Occurrence
         			TABLE_Audio.COL_Track[i] = i
         		END
         	END
         	
         	// Refresh the data in the status bar
         	DisplayStatusBar(“Del”)
         END
