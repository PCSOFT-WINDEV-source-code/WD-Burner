    name : BTN_Info
    identifier : 0x219cbec01100307c
    internal_properties : 
    properties :
     x : -90
     y : 278
     width : 48
     height : 48
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : 
        code : |1+
         CdDVDInserted is boolean = False
         
         BurnerSelect(gsGBurnerName)
         // Media used
         SWITCH BurnerMediaInfo(mediaFormat)
         	CASE mediaFormat_R
         		STC_MediaType = <§$0004§>
         		STC_Static1 = “CD-R”
         		CdDVDInserted = True // The inserted CD is usable
         	CASE mediaFormat_RW
         		STC_MediaType = <§$0005§>
         		STC_Static1 = “CD-RW”
         		CdDVDInserted = True	// The inserted CD is usable
         	CASE mediaFormat_Unusable
         		STC_MediaType = <§$0006§>
         		STC_Static1 = “Unusable”
         		CdDVDInserted = False	// The inserted CD is unusable
         	CASE mediaFormat_BD_R
         		STC_MediaType = “Blu-Ray Media”
         		STC_Static1 = “BD-R”
         		CdDVDInserted = True
         	CASE mediaFormat_BD_RE
         		STC_MediaType = “Rewritable Blu-Ray Media”
         		STC_Static1 = “BD-RE”
         		CdDVDInserted = True
         	CASE mediaFormat_BD_ROM
         		STC_MediaType = “Blu-Ray Media”
         		STC_Static1 = “Unwritable disk”
         		CdDVDInserted = False	// The inserted BD is unusable
         	CASE mediaFormat_DVDminusR
         		STC_MediaType = “DVD-R Media”
         		STC_Static1 = “DVD-R”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVDminusRW
         		STC_MediaType = “Rewritable DVD-R Media”
         		STC_Static1 = “DVD-RW”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVDminusR_DL
         		STC_MediaType = “Double-Layer Rewritable DVD-R Media”
         		STC_Static1 = “DVD-R”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVDplusR
         		STC_MediaType = “DVD+R Media”
         		STC_Static1 = “DVD+R”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVDplusRW
         		STC_MediaType = “Rewritable DVD+R Media”
         		STC_Static1 = “DVD+RW”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVDplusRW_DL
         		STC_MediaType = “Double-Layer Rewritable DVD+R Media”
         		STC_Static1 = “DVD+RW DL”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVDplusR_DL
         		STC_MediaType = “Double-Layer DVD+R Media”
         		STC_Static1 = “DVD+R DL”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVD_RAM
         		STC_MediaType = “DVD-RAM Media”
         		STC_Static1 = “DVD-RAM”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVD_RAM_HD
         		STC_MediaType = “HD DVD-RAM Media”
         		STC_Static1 = “DVD-RAM HD”
         		CdDVDInserted = True	// The DVD is usable
         	CASE mediaFormat_DVD_ROM
         		STC_MediaType = “DVD Media”
         		STC_Static1 = “Unwritable disk”
         		CdDVDInserted = False	// The inserted DVD is unusable
         	CASE mediaFormat_DVD_ROM_HD
         		STC_MediaType = “HD DVD Media”
         		STC_Static1 = “Unwritable disk”
         		CdDVDInserted = False	// The inserted DVD is unusable
         	CASE mediaFormat_DVD_R_HD
         		STC_MediaType = “HD DVD Media”
         		STC_Static1 = “DVD HD”
         		CdDVDInserted = True	// The DVD is usable
         END
         
         IF CdDVDInserted THEN
         	// Checks whether the CD/DVD is blank
         	IF BurnerMediaInfo(mediaBlank) THEN
         		EDT_CD_DVD_Blank = “Yes”
         	ELSE
         		EDT_CD_DVD_Blank = “No”
         	END
         	
         	// Checks the status of the burner
         	SWITCH BurnerStatus()
         		CASE burnerBurn 
         			STC_Status = <§$0001§>
         		// Burner disabled
         		CASE burnerInactive 
         			STC_Status =“No burner is used”
         		CASE burnerPrepareErasing
         			STC_Status = “Preparing to erase”
         		CASE burnerPrepareBurning
         			STC_Status = “Preparing to burn”
         	END
         
         	// Type of CD
         	SWITCH BurnerMediaInfo(mediaType)
         		CASE mediaType_CDDA_CDROM //audio
         			EDT_Type_of_Disk = “Audio”
         			// make the information about disk capacity invisible
         			GrpCapacity..Visible = False
         			// Modify the size of the window (hides the chart control)
         			WIN_DiskInfo..Width = 270
         			WIN_DiskInfo..Height = 366
         		CASE mediaType_CD_EXTRA // Data and audio
         			EDT_Type_of_Disk = “data and audio”
         		CASE mediaType_CD_I
         			EDT_Type_of_Disk = “data, audio and video”
         		CASE mediaType_CD_ROM_XA
         			EDT_Type_of_Disk = “data”
         			// make the information about disk capacity visible
         			GrpCapacity..Visible = True
         			WIN_DiskInfo..Width = WIN_DiskInfo..InitialWidth
         			WIN_DiskInfo..Height = WIN_DiskInfo..InitialHeight
         		CASE mediaType_Unknown
         			EDT_Type_of_Disk = “Other type of CD/DVD”
         		OTHER CASE
         			
         	END
         	
         	// Retrieve the data about the size of the CD/DVD
         	EDT_Number_of_sessions = BurnerMediaInfo(mediaNumberSessions)
         	EDT_Total_capacity     = (BurnerMediaInfo(mediaTotalSize)*0n2048)
         	EDT_Free_space	   = (BurnerMediaInfo(mediaFreeSize)*0n2048)
         	EDT_Space_Used	   = (BurnerMediaInfo(mediaSizeUsed)*0n2048)
         	
         	
         	
         	// Chart for viewing the disk space
         	// Name of the chart
         	sChartName is string = “MyChart”
         	
         	// Define the type of chart
         	grCreate(sChartName, gr3DSPie)
         	
         	// Destination of the chart
         	grDestinationControl(sChartName, IMG_Image2)
         	
         	// Font used for the chart title
         	sMyFont1 is font
         	sMyFont1 = FontCreate(“Arial”, 12, iBold+iUnderlined)
         	grTitleFont(sChartName, sMyFont1)
         	
         	// Other texts
         	sMyFont2 is font
         	sMyFont2 = FontCreate(“Arial”, 8, iNormal)
         	grAxisTitleFont(sChartName, sMyFont2)
         	grLabelFont(sChartName, sMyFont2)
         	grLegendFont(sChartName, sMyFont2)
         	
         	// Position of the legend
         	grLegend(sChartName, grNone)
         	
         	// Display the values
         	grLabel(sChartName, grShowValue, True)
         	
         	// Display the percentages
         	grLabel(sChartName, grShowPercent, True)
         	
         	// Animation of the chart
         	gr3DSParameter(sChartName, gr3DSPropAnimation, 14)
         	
         	// Start angle of the first section
         	grPieStartAngle(sChartName, 0)
         	
         	// Subscript of the section to pull out (the smallest one)
         	IF EDT_Space_Used < EDT_Free_space THEN
         		grPiePullOut(sChartName, 2, 30)
         	ELSE
         		grPiePullOut(sChartName, 1, 30)
         	END
         
         	
         	// Legend
         	grCategoryLabel(sChartName,1,“Free”)
         	grCategoryLabel(sChartName,2,“used”)
         	
         	// change the color
         	grSeriesColor(sChartName,1,iLightMagenta)
         	grSeriesColor(sChartName,2,iLightBlue)
         	
         	// data displayed
         	// display the percentage only if he size is not empty.
         	IF ((BurnerMediaInfo(mediaFreeSize)*0n2)/0n1024) <> 0 THEN grAddData(sChartName, 1,1, (BurnerMediaInfo(mediaFreeSize)*0n2)/0n1024)
         	IF ((BurnerMediaInfo(mediaSizeUsed)*0n2)/0n1024) <> 0 THEN grAddData(sChartName, 1,2, (BurnerMediaInfo(mediaSizeUsed)*0n2)/0n1024)
         	
         	
         		
         	// Draw the chart
         	grDraw(sChartName)	
         END
        type : 18
