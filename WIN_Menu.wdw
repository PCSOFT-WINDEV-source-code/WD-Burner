     type_code : 14
     code : |1+
      // Syntax:
      //FishEyeEffect ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procedure FishEyeEffect()
      
      // Reinitializes the fisheye effect
      CancelFishEyeEffectAll()
      
      x is int
      nx is int = MySelf..X
      
      
      //
      // Increases the size and the dimensions of the button
      //
      IF MySelf..Width <> 128 THEN
      	IF WinStatus(WIN_Bubble) <> NotFound THEN
      		Close(WIN_Bubble)
      	END	
      
      	FOR x = 80 TO 128 STEP 2
      		MySelf..X -= 1
      		MySelf..Y -= 1
      		MySelf..Width += 2
      		MySelf..Height += 2
      	END
      	
      	MySelf..Width = 128
      	MySelf..Height = 128
      
      	OpenChild(“WIN_Bubble”+NoFocus, MyWindow..X + MySelf..X, MyWindow..Y + MySelf..Y + MySelf..Height + 10, MySelf..Width, MySelf..Note)
      END
      
      
     type : 458752
