      // Syntax:
      //CancelFishEyeEffectAll ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      //	Except (default value=""): Optional, name of the button control whose fisheye effect must not be canceled
      Procedure CancelFishEyeEffectAll()
      
      	i is int
      
      	FOR i=1 _TO_ NUMBER_OPTIONS
      		IF MySelf..Name <> “OPTION” + i THEN
      			CancelFishEyeEffect({“OPTION” + i, indControl})
      		END
      	END
      
      
     type : 458752
   -
     name : FishEyeEffect
     internal_properties : CAAAAAgAAAA0j3DGSbDakcbmS2Wq6svu88CUvMJ/ea70/mAVVaAiv5IHgZypFrQ1zOSG5ZbUIZo+DgYbHqQxg74a8Wbs+gy9dQZwJJAsm64kTK7dpXL5YheEZDIWOwCfbQLdbcgVjSjUA5QIPZ5inZHsuoFojN2EIQcD1izBpLu0Obhi4VXrWjLsrhZTYsGcQJUHBbj4IaMlfplUz8Q=
