     type_code : 14
     code : |1+
      // Syntaxe :
      //CancelFishEyeEffect (<Button>)
      //
      // Paramètres :
      //	Button : Button control whose fisheye effect must be canceled
      
      Procedure CancelFishEyeEffect(Button)
      
      // Variables
      x is int
      nX is int = Button..X
      
      //
      // Reduces the size and the dimensions of the button
      //
      IF Button..Width <> 80 THEN
      
      	FOR x = 80 _TO_ 128 step 2
      		Button..X += 1
      		Button..Y += 1
      		Button..Width -= 2
      		Button..Height -= 2
      	END
      	
      	Button..Width = 80
      	Button..Height = 80	
      
      END
      
     type : 458752
   -
