      // Syntax:
      //CursorPosition ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      Procedure CursorPosition()
      
      	EndTimer()
      
      	nX, nY are int
      	nX = LoWord(CursorPos(cpScreen))
      	nY = HiWord(CursorPos(cpScreen))
      	
      	// If the window is not hovered by the mouse cursor
      	IF (MyWindow..X < nX < MyWindow..X + MyWindow..Width) = False OR ...
      	   (MyWindow..Y < nY < MyWindow..Y + MyWindow..Height) = False THEN
      	   
      		i is int
      	
      		// Reinitializes the fisheye effect for all
      		FOR i=1 _TO_ NUMBER_OPTIONS
      			CancelFishEyeEffect({“OPTION” + i, indControl})
      		END	   
      		
      		// Closes the tooltip
      		IF WinStatus(WIN_Bubble) <> NotFound THEN
      			Close(WIN_Bubble)
      		END			
      	   
      	END
      	
      	Timer(CursorPosition, 5)
     type : 458752
